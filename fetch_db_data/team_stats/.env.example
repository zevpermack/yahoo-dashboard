#Do not modify this file, copy it to .env and set your own values
# The host of your database
DB_HOST=localhost

# The name of your database
DB_NAME=your_db_name

# The user for your database, default users are usually postgres
DB_USER=postgres

# The password for your database user
DB_PASSWORD=your_db_password

# Set to True when running in development mode
DEVELOPMENT=True
